<UserControl x:Class="WpfDemo.TabSetting.TabSettingDemo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfDemo.TabSetting"
        mc:Ignorable="d">
    <StackPanel Orientation="Vertical">
        <Label Content="点击Tab键响应的顺序设置"></Label>
        <Button Content="Button1" TabIndex="0"></Button>
        <Button Content="Button2" TabIndex="1"></Button>
        <Button Content="Button3" TabIndex="2"></Button>
        <Button Content="Button4" TabIndex="5"></Button>
        <Button Content="Button5" TabIndex="3"></Button>
        <Button Content="Button6" TabIndex="4"></Button>
    </StackPanel>
</UserControl>
